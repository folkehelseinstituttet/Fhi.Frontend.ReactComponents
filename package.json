{
  "name": "@folkehelseinstituttet/react-components",
  "version": "1.0.0",
  "description": "Frontend components made with react for use in FHI",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run storybook",
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "babel": "npm run clean && cross-env NODE_ENV=production npx babel --extensions .ts,.tsx -d dist src --ignore **/*.test.tsx,**/*.stories.tsx --source-maps --copy-files --no-copy-ignored",
    "clean": "rm -rf dist",
    "build": "npm test && npm run eslint && npm run generateThemeVariables && npm run babel",
    "generateThemeVariables": "node src/components/FhiTheme/FhiVariableGenerator.ts > src/components/FhiTheme/FhiThemeVariables.json",
    "eslint": "npx eslint src --ext .ts --ext .tsx",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@folkehelseinstituttet/style": "^3.2.3",
    "react": ">=18",
    "react-dom": ">=18",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@folkehelseinstituttet/style": "^3.2.3",
    "@mdx-js/react": "^2.1.1",
    "@storybook/addon-actions": "^6.5.0-beta.0",
    "@storybook/addon-essentials": "^6.5.0-beta.0",
    "@storybook/addon-interactions": "^6.5.0-beta.0",
    "@storybook/addon-links": "^6.5.0-beta.0",
    "@storybook/builder-webpack5": "^6.5.0-beta.0",
    "@storybook/manager-webpack5": "^6.5.0-beta.0",
    "@storybook/react": "^6.5.0-beta.0",
    "@storybook/testing-library": "^0.0.11",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.0.1",
    "lodash.camelcase": "^4.3.0",
    "node-sass": "^7.0.1",
    "node-sass-variables": "^0.0.4",
    "npm": "^8.8.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-test-renderer": "^18.1.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.5",
    "tslib": "^2.4.0"
  }
}
